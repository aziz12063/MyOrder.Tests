{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2",
      "Authentication": {
        "Schemes": "Negotiate"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Routes": {
    "Basket": "/?BasketId={basketId}",
    "Error": "/error"
  }
}
