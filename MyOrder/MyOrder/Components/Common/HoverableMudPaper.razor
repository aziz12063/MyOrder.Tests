@inherits HoverableMudPaperBase

<MudPaper 
          @onmouseenter="MouseEnterHandler"
          @onmouseleave="MouseLeaveHandler"
          Elevation="@currentElevation"
          Class="@Class"
          Style="background: linear-gradient(135deg, #ffffff, #f5f5f5);">
    @ChildContent
</MudPaper>