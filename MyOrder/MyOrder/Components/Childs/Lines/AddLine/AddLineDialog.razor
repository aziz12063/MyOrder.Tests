<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h5">Ajout d'article</MudText>
    </TitleContent>
    <DialogContent>
        <MudTabs Class="tabfix"
                 AlwaysShowScrollButtons="false"
                 Centered="true">
            <MudTabPanel Text="Ajout" Icon="@Icons.Material.Filled.AddCircle">
                <AddLineTab />
            </MudTabPanel>
            <MudTabPanel Text="Top Ventes" Icon="@Icons.Material.Filled.TrendingUp">
                <MudText>Top Ventes</MudText>
            </MudTabPanel>
            <MudTabPanel Text="Commandes" Icon="@Icons.Material.Filled.ShoppingCart">
                <MudText>Commandes</MudText>
            </MudTabPanel>
            <MudTabPanel Text="Devis" Icon="@Icons.Material.Filled.Description">
                <MudText>Devis</MudText>
            </MudTabPanel>
            <MudTabPanel Text="Recherche" Icon="@Icons.Material.Filled.Search">
                <MudText>Recherche</MudText>
            </MudTabPanel>
            <MudTabPanel Text="Saisie Libre" Icon="@Icons.Material.Filled.Edit">
                <MudText>Saisie Libre</MudText>
            </MudTabPanel>
        </MudTabs>
    </DialogContent>
    <DialogActions>

    </DialogActions>
</MudDialog>

@code {
    [Inject]
    public ILogger<AddLineDialog> Logger { get; set; }
}